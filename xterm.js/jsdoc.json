{
  "source": {
    "include": [
      "src/"
    ],
    "exclude": [
      "src/test/"
    ],
    "excludePattern": "src/.+\\.test\\.(js|ts)$"
  },
  "opts": {
    "readme": "README.md",
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
